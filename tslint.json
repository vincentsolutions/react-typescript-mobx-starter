{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ],
    "alwaysShowRuleFailuresAsWarnings": true,
    "experimentalDecorators": true
  },
  "rules": {
    "no-console": false
  },
  "defaultSeverity": "warning"
}